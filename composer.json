{
	"name": "web-vision/mime-converter",
	"type": "typo3-cms-extension",
	"require": {
		"typo3/cms-core": "^11.5",
		"ext-imagick": "*",
		"ext-fileinfo": "*"
	},
	"suggest": {
		"ext-imagick": "*",
		"ext-gmagick": "*"
	},
	"autoload": {
		"psr-4": {
			"WebVision\\MimeConverter\\": "Classes"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"WebVision\\MimeConverter\\Tests\\": "Tests"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "mime_converter"
		}
	}
}
